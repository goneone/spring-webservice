{{>layout/header2}}
    <div class="col-md-12">
        <div class="card card-container">
            <img
                    id="profile-img"
                    src="//ssl.gstatic.com/accounts/ui/avatar_2x.png"
                    class="profile-img-card"
            />
            <form name="form">
                    <div class="form-group">
                        <label for="username">아이디</label>
                        <input
                                type="text"
                                class="form-control"
                                name="username"
                        />
                    </div>
                    <div class="form-group">
                        <label for="password">비밀번호</label>
                        <input
                                type="text"
                                class="form-control"
                                name="password"
                        />
                    </div>
                    <div class="form-group">
                        <label for="passwordConfirm">비밀번호 재확인</label>
                        <input
                                type="text"
                                class="form-control"
                                name="passwordConfirm"
                        />
                    </div>
                    <div class="form-group">
                        <label for="name">이름</label>
                        <input
                                type="text"
                                class="form-control"
                                name="passwordConfirm"
                        />
                    </div>
                    <div class="form-group">
                        <label for="gender">성별</label>
                        <select name ="gender">
                            <option value="man" selected="selected">남자</option>
                            <option value="woman">여자</option>
                        </select>
                    </div>

                    <div class="form-group">
                        <label for="email">Email</label>
                        <input
                                type="email"
                                class="form-control"
                                name="email"
                        />
                    </div>

                    <div class="form-group">
                        <button class="btn btn-primary btn-block">가입하기</button>
                    </div>
            </form>
        </div>
    </div>
{{>layout/footer}}


